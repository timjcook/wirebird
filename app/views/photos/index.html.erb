<div class="content">
  <div class="gallery">
    <% @photos.each() do |photo| %>
      <div class='photo round'>
        <% info = flickr.photos.getInfo(:photo_id => photo.id)%>
        <%= link_to(image_tag(FlickRaw.url_b(info), :alt => "Wire Bird"), FlickRaw.url_b(info), :rel => "prettyPhoto", :title => "Wire Bird") %>
      </div>
    <% end %>
  </div>
</div>
